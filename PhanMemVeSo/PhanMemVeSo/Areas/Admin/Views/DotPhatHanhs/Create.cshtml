@model Model.ViewModels.DotPhatHanh
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

<div class="form-horizontal">
    <h4>Đợt phát hành ngày @ViewBag.NgayPhatHanh vé số ViewBag.LoaiVeSo</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })



    @foreach (var item in Model.PhieuPhatHanhs)
    {
        <div class="form-group">
            <label class = "control-label col-md-2">Đại lý: @item.DaiLyId.</label>
            <label class = "control-label col-md-2">So luong phat:</label>
            <input />
        </div>
    }

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
</div>
}

